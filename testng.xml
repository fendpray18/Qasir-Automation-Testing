<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<!--Identify the listener file we created -->
	<listeners>
		<listener class-name="qasirTesting.TestListener" />
	</listeners>
	<test name="Scenario Full Testing">
		<classes>
			<class name="qasirTesting.NavigateFullTesting" />
